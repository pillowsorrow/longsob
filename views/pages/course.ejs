<!DOCTYPE html>
<html lang="<%= i18n_lang %>">

<head>
  <%- include('../partials/head'); -%>
  <%- include('../partials/nav'); -%>
</head>

<body>
  <%- include('../partials/header'); -%>
    <!-- ========== MAIN CONTENT ========== -->
    <main id="content" role="main">
      <div class="bg-theme-second">
        <%- include('../partials/breadcrumb'); -%>
      </div>
      <!-- End Viwer Section -->


      <!-- Pricing Section -->
      <div class="container space-top-1">
        <!-- space-top-3 -->
        <div class="row d-flex">
          <div class="col-md-3 hidden-xs pr-md-0">
            <h3 class="h4 mb-4 text-primary-theme heading-subject-text">
              <%= translation.aside_course %>
            </h3>
            <article class="card border-0 mb-3 transition-3d-hover">
              <div class="card-body p-0 course-aside">
                <div class="media">
                  <div class="u-avatar">
                    <i class="fas fa-book font-size-3 text-primary-theme"></i>
                  </div>
                  <div class="media-body">
                    <h4 class="h5 font-weight-normal mb-0">
                      <a href="#" class="text-black-70"> </a>
                    </h4>
                  </div>
                </div>
         
              </div>
            </article>
          </div>
          <div class="col-md-9 ">
            <div class="mb-2 ">
              <!-- text-center -->
              <h4 class="h4 mb-4 text-primary-theme heading-subject-text">#<!-- {{group-subject}}--></h4>
            </div>
            <div class="row" id="course-block">
              <div class="col-12 course-block-zero"><%= translation.zero_result %></div>
              <div class="col-sm-12 col-md-6 col-lg-4 pb-3 course-block-detail">
                <!-- Product -->
                <div class="card">
                  <div class="position-relative  img-hover">
                      <picture>
                        <img src="/images/blank.png" alt="#" loading="lazy" decoding="async">
                      </picture>
                  </div>
                  <div class="card-body pt-4 px-3 pb-0 bg-theme-second">
                    <div class="mb-2">
                      <a href="#">
                        <h2 class="h5 mb-0 fix-line h-65-px text-black-70 mb-1">#</h2>
                        <small class="text-secondary fix-line-50 mobile-none text-line-clamp d-none"> </small>
                        <small class="text-secondary fix-line-50 mobile-none text-line-clamp"></small>
                        <%= translation.price%> <var class="price"></var> <%= translation.baht%>
                      </a>
                    </div>
                  </div>
                  <div class="card-footer border-0 pt-0 pb-3 px-3 bg-theme-second text-right">
                    <p class="m-0 pt-3">
                      <a class="btn btn-sm btn-primary-theme transition-3d-hover"
                        href="#" ><%= translation.menu_text_login_register2 %></a>
                    </p>
                  </div>
                </div>
                <!-- End Product -->
              </div>

            </div>
          </div>
        </div>
      </div>
      <!-- End Pricing Section -->

      <%- include('../partials/aside'); -%>
    </main>
    <!-- ========== END MAIN CONTENT ========== -->
    <%- include('../partials/footer'); -%>
    <%- include('../partials/script'); -%>
    <script async defer src="/javascripts/course.js"></script>
    <script>
      let course_group = <%= course_group %>;
      const registered_txt = '<%= translation.registered_txt %>';
    </script>
</body>

</html>